<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.VBox?>

<BorderPane prefHeight="200.0" prefWidth="200.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Gui.Controllers.Achievement">
   <top>
      <Label text="Plays" BorderPane.alignment="CENTER" />
   </top>
   <center>
      <VBox prefHeight="200.0" prefWidth="100.0">
         <children>
            <CheckBox fx:id="cb3" contentDisplay="CENTER" disable="true" mnemonicParsing="false" text="25 Plays" />
            <CheckBox fx:id="cb2" contentDisplay="CENTER" disable="true" layoutX="10.0" layoutY="10.0" mnemonicParsing="false" text="5 Plays" />
            <CheckBox fx:id="cb1" contentDisplay="CENTER" disable="true" layoutX="10.0" layoutY="28.0" mnemonicParsing="false" text="1 Plays" />
         </children>
      </VBox>
   </center>
   <bottom>
      <Button mnemonicParsing="false" onAction="#back" text="Back" BorderPane.alignment="CENTER" />
   </bottom>
</BorderPane>
